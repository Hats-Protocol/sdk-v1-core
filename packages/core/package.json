{
  "name": "@hatsprotocol/sdk-v1-core",
  "version": "0.10.0",
  "license": "MIT",
  "author": "Hats-Protocol",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hats-Protocol/sdk-v1-core"
  },
  "main": "dist/hats.cjs.js",
  "module": "dist/hats.esm.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "hats-protocol",
    "hats",
    "ethereum",
    "eth"
  ],
  "scripts": {
